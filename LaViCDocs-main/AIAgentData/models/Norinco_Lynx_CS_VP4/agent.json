    "oodas": [
      {
        "scriptId": "ooda_228888467585974272",
        "oodaName": "避障",
        "oodaNameI18n": "避障",
        "scriptLang": 0,
        "oodaVersion": 1,
        "oodaActivated": true,
        "oodaUsageFilter": 8,
        "oodaScript": [
          "{",
          "//排除自身感知域发现自身实体",
          "if(Agent.event.aid != Agent.event.self_id)",
          "{",
          "    return;",
          "}",
          "//获取对方优先级",
          "var mission_level_a = Agent.variables[\"missionLevel\"].get();",
          "var agent_b = system_other_agent(Agent.event.bid);",
          "if(system_exists(agent_b)){",
          "    var keyword = agent_b._agentKeyword();",
          "    if(keyword != \"SUV\"){",
          "        return;",
          "    }",
          "    var mission_level_b = agent_b.variables[\"missionLevel\"].get();",
          "    if(mission_level_a < mission_level_b){",
          "            if (Agent.event.event_type == E_EVENT_MODE_ADD) ",
          "            {",
          "                var _bid = Agent.event.bid.to_string();",
          "                if(Agent.variables[\"priorVehicles\"].get().contains(_bid) == false){",
          "                    Agent.variables[\"priorVehicles\"].get().push_back(_bid);",
          "                }",
          "            }",
          "            else if (Agent.event.event_type == E_EVENT_MODE_UPDATE) ",
          "            {",
          "            }",
          "            else if (Agent.event.event_type == E_EVENT_MODE_REMOVE) ",
          "            {   ",
          "                var _bid = Agent.event.bid.to_string();",
          "                //print(\"remove bid\");",
          "                //print(_bid);",
          "                Agent.variables[\"priorVehicles\"].get().remove(_bid);",
          "            }",
          "            //print(Agent.variables[\"priorVehicles\"].get());",
          "            if(Agent.variables[\"priorVehicles\"].get().size() == 0){",
          "                Agent.action_do(\"start\", 1000);",
          "            } else {",
          "                Agent.action_do(\"stop\");",
          "            }",
          "    }",
          "}",
          "}",
          ""
        ],
        "scriptTalk": [],
        "oodaDesc": "当两车距离小于安全阈值时，系统比较任务等级，低优先级车辆暂停行驶，待高优先级车辆通过且距离恢复安全值后继续运行。"
      },
      {
        "scriptId": "ooda_228933062615662592",
        "oodaName": "充能",
        "oodaNameI18n": "充能",
        "scriptLang": 0,
        "oodaVersion": 1,
        "oodaActivated": true,
        "oodaUsageFilter": 8,
        "oodaScript": [
          "{",
          "//排除自身感知域发现自身实体",
          "if(Agent.event.aid != Agent.event.self_id)",
          "{",
          "    return;",
          "}",
          "var agent_b = system_other_agent(Agent.event.bid);",
          "if(system_exists(agent_b)){",
          "    var keyword = agent_b._agentKeyword();",
          "    if(keyword != \"Charge\"){",
          "        return;",
          "    }",
          "    if (Agent.event.event_type == E_EVENT_MODE_ADD) ",
          "    {",
          "        // 开始充能",
          "        Agent.variables[\"energyCost\"].set(3.0);",
          "    }",
          "    else if (Agent.event.event_type == E_EVENT_MODE_UPDATE) ",
          "    {",
          "        //vehicleEnergy",
          "    ",
          "        var energyMax = Agent.variables[\"maxEnergy\"].get();",
          "        var energy = Agent.variables[\"vehicleEnergy\"].get();",
          "        var per = energy/energyMax * 100;",
          "        if(per > 95){",
          "            Agent.action_do(\"start\", 1000);",
          "            Agent.variables[\"energyCost\"].set(-1.0);",
          "            //print(\"stop charge\");",
          "        }",
          "        //print(per);",
          "        ",
          "    }",
          "    else if (Agent.event.event_type == E_EVENT_MODE_REMOVE) ",
          "    {",
