      {
        "scriptId": "action_228930721729957888",
        "axnKeyword": "ZrNnIQcj",
        "axnName": "能量消耗",
        "axnNameI18n": "能量消耗",
        "i18nLabels": [],
        "axnIcon": "http://192.168.31.218:9000/lavic/bebbe9e7-ae63-45e8-8ccf-88559ee88a9daction.png",
        "vardefs": [],
        "scriptLang": 0,
        "axnVersion": 0,
        "axnActivated": true,
        "axnRunningPlan": 1000,
        "axnAsync": false,
        "axnRunningConds": [
          "return true;"
        ],
        "axnScript": [
          "{",
          "var energy = Agent.variables[\"vehicleEnergy\"].get();",
          "var energyMax = Agent.variables[\"maxEnergy\"].get();",
          "var energy_cost = Agent.variables[\"energyCost\"].get();",
          "var _energy = energy + energy_cost;",
          "if(_energy < 0){",
          "    _energy = 0;",
          "    Agent.action_do(\"stop\");",
          "}",
          "",
          "if(energy > energyMax){",
          "    _energy = energyMax;",
          "}",
          "",
          "//print(_energy);",
          "//print(\"testa\");",
          "",
          "Agent.variables[\"vehicleEnergy\"].set(_energy);",
          "}"
        ],
